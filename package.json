{
  "name": "tanq-language-support",
  "displayName": "TanQ for VS Code",
  "description": "Adds syntax highlighting and other features for .tanq files.",
  "publisher": "tanqory",
  "version": "0.0.2",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.102.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tanqory/tanq-language-support.git"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "tanq",
        "aliases": ["TanQ", "tanq"],
        "extensions": [".tanq"],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/tanq-light.svg",
          "dark": "./icons/tanq-dark.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "tanq",
        "scopeName": "source.tanq",
        "path": "./syntaxes/tanq.tmLanguage.json"
      }
    ]
  }
}